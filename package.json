{
  "name": "obbba-household-by-household",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:github": "vite build",
    "build:policyengine": "BASE_PATH=/us/obbba-household-by-household vite build",
    "preview": "vite preview",
    "preview:policyengine": "BASE_PATH=/us/obbba-household-by-household vite preview",
    "serve:policyengine": "node serve-policyengine.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write \"src/**/*.{js,svelte,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,svelte,json,css}\"",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "deploy": "npm run build && npx gh-pages -d build -t true"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/svelte": "^5.2.8",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.3.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "dependencies": {
    "d3": "^7.9.0",
    "papaparse": "^5.5.3"
  }
}
